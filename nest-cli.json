{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/notification-microservices/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/notification-microservices/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/notification-microservices",
  "projects": {
    "notification-microservices": {
      "type": "application",
      "root": "apps/notification-microservices",
      "entryFile": "main",
      "sourceRoot": "apps/notification-microservices/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-microservices/tsconfig.app.json"
      }
    },
    "consumers": {
      "type": "application",
      "root": "apps/consumers",
      "entryFile": "main",
      "sourceRoot": "apps/consumers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/consumers/tsconfig.app.json"
      }
    }
  }
}